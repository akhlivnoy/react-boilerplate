{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ESNext", "ES6"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "#assets": ["./src/assets"],
      "#assets/*": ["./src/assets/*"],
      "#components": ["./src/components"],
      "#components/*": ["./src/components/*"],
      "#constants": ["./src/constants"],
      "#constants/*": ["./src/constants/*"],
      "#hooks": ["./src/hooks"],
      "#hooks/*": ["./src/hooks/*"],
      "#models": ["./src/models"],
      "#models/*": ["./src/models/*"],
      "#navigation": ["./src/navigation"],
      "#navigation/*": ["./src/navigation/*"],
      "#redux": ["./src/redux"],
      "#redux/*": ["./src/redux/*"],
      "#pages": ["./src/pages"],
      "#pages/*": ["./src/pages/*"],
      "#services": ["./src/services"],
      "#services/*": ["./src/services/*"],
      "#themes": ["./src/themes"],
      "#themes/*": ["./src/themes/*"],
      "#types": ["./src/types"],
      "#types/*": ["./src/types/*"],
      "#utils": ["./src/utils"],
      "#utils/*": ["./src/utils/*"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  },
  "include": ["src"]
}
